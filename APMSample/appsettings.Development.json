{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning",
        "Elastic.Transport": "Warning",
        "Elastic.Apm": "Warning",
        "RequestPipelineDiagnosticsListener": "Warning",
        "HttpConnectionDiagnosticsListener": "Warning",
        "SerializerDiagnosticsListener": "Warning"
      }
    }
  },
  "ElasticApm": {
    "ServerUrl": "http://localhost:8200",
    "ServiceName": "MyDotNet8App",
    "ServiceVersion": "1.0.0",
    "Environment": "Development",
    "TransactionSampleRate": 1.0,
    "LogLevel": "Debug"
  },
  "Elasticsearch": {
    "Uri": "http://localhost:9200"
  },
  "AllowedHosts": "*"
}