{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ElasticApm": {
    "ServerUrl": "http://localhost:8200",
    "ServiceName": "MyDotNet8App",
    "ServiceVersion": "1.0.0",
    "Environment": "Development",
    "LogLevel": "Debug",
    "FlushInterval": "0s", // 不准等待，立刻送
    "MaxBatchEventCount": 1 // 只要有 1 筆資料就送
  },
  "AllowedHosts": "*"
}